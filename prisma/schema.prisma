generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  //url = "postgres://postgres:postgrespw@localhost:49153"
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  cpf      String   @unique
  password String
  coletor  Coletor?
  // Produtor Produtor[]

  // Produtor   Produtor? @relation(fields: [produtorId], references: [id])
  // produtorId Int?
}

model Coletor {
  id        Int   @id @default(autoincrement())
  user      User? @relation(fields: [userId], references: [id])
  userId    Int? @unique
  pinto     Int

  // User User[]
  // corridaId Int?
  // Veiculo   Veiculo?  @relation(fields: [veiculoId], references: [id])
  // veiculoId Int?
  // Corrida   Corrida[]
  // Corrida   Corrida[]
  // Veiculo   Veiculo?  @relation(fields: [veiculoId], references: [id])
  // veiculoId Int?
}

model Produtor {
  id        Int    @id @default(autoincrement())
  local     Local?
  // Veiculo   Veiculo?  @relation(fields: [veiculoId], references: [id])
  veiculoId Int?
  corridaId Int?
  // Corrida   Corrida[]
  // User      User?     @relation(fields: [userId], references: [id])
  userId    Int?

  // User User[]
}

model Local {
  id          Int    @id @default(autoincrement())
  tipoLixo    String
  cep         String
  estado      String
  municipio   String
  bairro      String
  rua         String
  complemento String

  Produtor   Produtor @relation(fields: [produtorId], references: [id])
  produtorId Int      @unique
}

model Corrida {
  id         Int      @id @default(autoincrement())
