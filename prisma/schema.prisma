generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  //url = "postgres://postgres:postgrespw@localhost:49153"
}

model User {
  id    String     @default(uuid()) @id
  emailPhone String  @unique
  name  String
  cpfCnpj String
}